{
    "name": "melan",
    "displayName": "MeLan",
    "description": "Language extension for MeLan: the meta-language with Python backend.",
    "version": "0.1.3",
    "publisher": "jhadida",
    "license": "MIT",
    "homepage": "https://github.com/jhadida/melan-vscode",
    "repository": {
        "type": "git",
        "url": "https://github.com/jhadida/melan-vscode.git"
    },
    "engines": {
        "vscode": "^1.44.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "keywords": [
        "melan",
        "python",
        "meta-language",
        "text-processor"
    ],
    "contributes": {
        "languages": [
            {
                "id": "melan",
                "aliases": [
                    "MeLan",
                    "melan"
                ],
                "extensions": [
                    ".mel"
                ],
                "configuration": "./syntax/configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "melan",
                "scopeName": "text.mel",
                "path": "./syntax/melan.tmLanguage.json",
                "embeddedLanguages": {
                    "source.json": "json"
                }
            }
        ]
    }
}
