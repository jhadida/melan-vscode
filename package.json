{
    "name": "melan",
    "displayName": "MeLan",
    "description": "The meta-language with Python backend.",
    "version": "0.1.0",
    "engines": {
        "vscode": "^1.44.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "melan",
            "aliases": ["MeLan", "melan"],
            "extensions": [".mel"],
            "configuration": "./syntax/configuration.json"
        }],
        "grammars": [{
            "language": "melan",
            "scopeName": "source.mel",
            "path": "./syntax/melan.tmLanguage.json"
        }]
    }
}